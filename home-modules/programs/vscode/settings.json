  "dance.modes": {
    "input": {
      "inheritFrom": "insert"
    },
    "insert": {
      "cursorStyle": "underline",
      "lineNumbers": "inherit",
      "selectionBehavior": "character"
    },
    "normal": {
      "cursorStyle": "block",
      "lineNumbers": "inherit",
      "selectionBehavior": "character"
    },
    "select": {
      "inheritFrom": "normal",
      "onLeaveMode": [
        [
          ".selections.save",
          {
            "register": " ^",
            "style": {
              "borderColor": "$editor.selectionBackground",
              "borderRadius": "1px",
              "borderStyle": "solid",
              "borderWidth": "2px"
            },
            "until": [
              [
                "mode-did-change",
                {
                  "include": [
                    "normal",
                    "select"
                  ]
                }
              ],
              [
                "selections-did-change"
              ]
            ]
          }
        ]
      ]
    }
  },
